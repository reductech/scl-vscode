{
    "name": "reductech-scl",
    "displayName": "SCL",
    "description": "Sequence Configuration Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:scl"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "scl",
                "aliases": [
                    "SCL",
                    "scl"
                ],
                "extensions": [
                    ".scl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "scl",
                "scopeName": "source.scl",
                "path": "./syntaxes/scl.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "vscode-languageclient": "^5.1.1",
        "vscode-jsonrpc": "^4.0.0",
        "@types/node": "^8.10.25"
    }
}
